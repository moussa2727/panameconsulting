{
  "name": "backend",
  "version": "1.0.0",
  "description": "NestJS backend for Paname Consulting's Application",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch --verbose",
    "prod": "node dist/main",
    "docker:build": "docker build -t backend .",
    "docker:run": "docker run -p 10000:10000 backend",  
    "start:prod": "node dist/main",
    "docker:prod": "docker run -p 10000:10000 --env-file .env.production backend",
    "deploy:setup": "npm run build && npm run docker:build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit --watch"
  },
  "keywords": [
    "nestjs",
    "mongodb",
    "jwt",
    "authentication"
  ],
  "author": "Moussa Sangar√© <moussa@example.com>",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "10.2.7",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.2.7",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "mongoose": "^8.20.1",
    "multer": "^2.0.2",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.4",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/bcrypt": "^5.0.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.25",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "@nestjs/core": "10.2.7",
    "@nestjs/common": "10.2.7",
    "typescript": "5.3.3",
    "eslint": "^9.4.0",
    "@typescript-eslint/*": "^7.8.0"
  }
}