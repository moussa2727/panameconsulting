{
  "name": "paname-consulting-app",
  "description": "Paname Consulting - Application web pour la présentation des services ainsi que l'entreprise et la gestion des rendez-vous, utilisateurs et procedures d'etudes à l'étranger , voyage d'affaires et de tourisme",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:vercel": "vite build --outDir dist",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "audit": "npm audit",
    "ci": "npm run type-check && npm run lint && npm run test:coverage && npm run build",
    "prepare": "echo \"Skipping husky for deployment\" || exit 0",
    "clean": "rm -rf node_modules dist .vite coverage",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@headlessui/react": "^1.7.19",
    "@mantine/core": "^7.17.8",
    "@mantine/hooks": "^7.17.8",
    "@tanstack/react-query": "^5.8.4",
    "@tremor/react": "^3.14.2",
    "aos": "^2.3.4",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "class-transformer": "^0.5.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "framer-motion": "^10.18.0",
    "joi": "^18.0.1",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.553.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.48.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "react-toastify": "^9.1.3",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "swr": "^2.2.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/node": "^20.19.24",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.3.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}