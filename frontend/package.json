{
  "name": "paname-consulting-app",
  "description": "Paname Consulting - Application web pour la présentation des services ainsi que l'entreprise et la gestion des rendez-vous, utilisateurs et procedures d'etudes à l'étranger , voyage d'affaires et de tourisme",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf node_modules dist .vite coverage",
    "reset": "pnpm run clean && pnpm install"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@headlessui/react": "^2.2.9",
    "@mantine/core": "^7.14.0",
    "@mantine/hooks": "^7.14.0",
    "@radix-ui/react-toast": "1.2.15",
    "@tanstack/react-query": "^5.62.3",
    "@tremor/react": "^3.18.7",
    "aos": "^2.3.4",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "class-transformer": "^0.5.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.0.7",
    "joi": "^17.13.3",
    "js-cookie": "3.0.5",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.468.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.0",
    "react-toastify": "^9.1.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/js-cookie": "3.0.6",
    "@types/node": "^22.10.5",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.5.2",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}