{
  "name": "paname-consulting-app",
  "description": "React application for appointments management, user administration and global application administration",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "audit": "pnpm audit",
    "ci": "pnpm type-check && pnpm lint && pnpm test:coverage && pnpm build",
    "prepare": "husky install",
    "clean": "rm -rf node_modules dist .vite coverage",
    "reset": "pnpm clean && pnpm install"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.0",
    "@headlessui/react": "^2.0.0",
    "@mantine/core": "^7.8.0",
    "@mantine/hooks": "^7.8.0",
    "@tanstack/react-query": "^5.0.0",
    "@tremor/react": "^3.0.0",
    "aos": "^2.3.4",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "class-transformer": "^0.5.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "framer-motion": "^10.16.4",
    "joi": "^17.10.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.0",
    "react-hook-form": "^7.50.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.21.0",
    "react-toastify": "^9.1.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "swr": "^2.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/date-fns": "2.6.3",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "24.3.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "6.0.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.8",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}