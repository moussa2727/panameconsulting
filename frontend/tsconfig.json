{
   "compilerOptions": {
      "target": "es5",
      "lib": [
         "dom",
         "dom.iterable",
         "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": ".",
      "paths": {
         "@/*": [
            "src/*"
         ]
      }
   },
   "include": [
      "**/*.ts",
      "**/*.tsx"
, "src/main.jsx", "src/pages/Rendez-Vous.jsx", "src/pages/Accueil.jsx", "src/components/DestinationQuiz.tsx"   ],
   "exclude": [
      "node_modules"
   ]
}